{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
        "@typescript-eslint",
        "jest"
    ],
    "extends": [
        "airbnb-typescript",
        "plugin:jest/all"
    ],
    "parserOptions": {
        "project": "tsconfig.json"
    },
    "rules": {
        "max-len": 0,
        "no-plusplus": 0,
        "import/prefer-default-export": "off"
    }
}